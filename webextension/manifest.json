{

  "manifest_version": 2,
  "name": "Opquast Desktop",
  "version": "2.1.0",
  "author": "Temesis SAS",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://desktop.opquast.com/",

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png"
  },
  "background": {
    "scripts": [
      "legacy.js",
      "browser-ui.js",
      "main.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://reporting.opquast.com; object-src 'self'",
  "default_locale": "en",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "downloads"
  ],
  "applications": {
    "gecko": {
      "id": "opquast-desktop-signed@temesis.com",
      "strict_min_version": "51.0"
    }
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png"
    },
    "default_title": "Opquast Desktop",
    "browser_style" : true
  },
  "commands": {
    "opquast-open": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+U"
      },
      "description": "Opquast Desktop"
    }
  }

}